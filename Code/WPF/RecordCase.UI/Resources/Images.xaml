<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <BitmapImage x:Key="BackgroundImage" UriSource="pack://application:,,,/Resources\Images\background.png"/>
    <BitmapImage x:Key="LogoImage" UriSource="pack://application:,,,/Resources\Images\logo.png"/>
    <ImageBrush x:Key="BackgroundBrush" ImageSource="{StaticResource BackgroundImage}"></ImageBrush>
</ResourceDictionary>